{
  "name": "Wordle Meaning",
  "version": "1.0.1",
  "description": "Search Wordle of the day meaning",
  "manifest_version": 3,
  "author": "github.com/imedgar",
  "action": {
    "default_popup": "index.html",
    "default_title": "Wordle Meaning"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.nytimes.com/games/wordle/index.html"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/logo_16.png",
    "32": "assets/logo_32.png",
    "48": "assets/logo_48.png",
    "128": "assets/logo_128.png"
  }
}
